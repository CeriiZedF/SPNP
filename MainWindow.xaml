<Window x:Class="SPNP.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SPNP"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="300">
    <StackPanel Orientation="Vertical">
        <Button Content="Багапоточність"
                x:Name="ThreadingButton"
                Margin="20 7"
                Click="ThreadingButton_Click"></Button>
        <Button Content="Синхронізація"
                x:Name="SynchroButton"
                Margin="20 7"
                Click="SynchroButton_Click"></Button>
        <Button Content="Багатозадачність"
                 x:Name="TaskWindow"
                 Margin="20 7"
                 Click="TaskWindow_Click"></Button>
        <Button Content="Скасування задач"
                 x:Name="Canceling"
                 Margin="20 7"
                 Click="Canceling_Click"></Button>
        <Button Content="Процеси"
                 x:Name="ProcessButton"
                 Margin="20 7"
                 Click="ProcessButton_Click"></Button>
        <Button Content="Нитки. Продовження"
                x:Name="ChainingButton"
                Margin="20 7"
                Click="ChainingButton_Click"></Button>
    </StackPanel>
</Window>
